			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="255"/>
		
		<property 
			name="paraName" 
			column="paraName" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="255"/>
		
		<property 
			name="paraType" 
			column="paraType" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="255"/>
		
		<property 
			name="paraValue" 
			column="paraValue" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="255"/>
		
		<property 
			name="status" 
			column="status" 
			update="true"
            insert="true"
            type="java.lang.String"
			not-null="false" 
			unique="false" 
			length="255"/>
		
		<!-- One-To-Many attributes -->

		<!-- Many-To-One attributes -->

		<!-- Many-To-Many attributes -->
    </class>
</hibernate-mapping>

