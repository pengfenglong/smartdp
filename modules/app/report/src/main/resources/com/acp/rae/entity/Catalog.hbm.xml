<?xml version="1.0"?> 
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.acp.rae.entity.Catalog" table="T_ACP_RAE_CATALOG">
		<id name="id">
			<generator class="uuid"></generator>
		</id>
		<property name="parentId" column="parentid" type="java.lang.String"></property>
		<property name="name" column="name" type="java.lang.String"></property>
		<property name="description" column="description" type="java.lang.String"></property>
	</class>
</hibernate-mapping>